<div id="field">
	<div id="divListPlayers"> 
		<input class="inp" id="iSearch" placeholder="**SEARCH**" type="text" onkeyup="search()" size=16/>
		<div class="tit" id="titFriends">**FRIENDS**</div>
		<div id="listFriends"></div>
		<div class="tit">**USERS**</div>
		<div id="listPlayers"></div>
	</div>
	<div id="menu"></div>
	
	<div id="divStart">
		<div class="box" id="helpStart">**HELPSTART**</div>
		<div class="box" id="start">
			<span>**INVPLS**</span>
			<div id="startBox"></div>
			<div class="but" id="bStart" onclick="startGame()">**START**</div>
		</div>
		<div class="box" id="invites">
			<span>**YOURINV**</span>
			<div id="invBox"></div>
		</div>
		<div id="divChat">
			<div class="tit">**CHAT**</div>
			<div id="chatArea">
				<textarea  class="inp" id="areaMess" placeholder="**WRITEMESS**" type="text" maxlength=256/>
				<div class="but" id="bSendMess">**SEND**</div>
				<div id="sp"></div>				
			</div>
			<div id="messageArea"></div>
		</div>
	</div>
