<div id="divlogin">	
	<table id="tabLogin" align="center">
		<tr  class="lineTabLog">
			<td align="right">**LOGIN** :</td>
			<td align="left"><input class="inp" type="text" id="iLogin"  size="16" maxlength="32" autofocus="true"/></td>
			<td><div class="message errLogin" id="eLogin">**LOGINNOT**</div></td>
			<td><div class="message errLogin" id="eActive">**EACTIVATION**</div></td> 
		</tr>
		<tr  class="lineTabLog">
			<td align="right">**PASSWORD** :</td>
			<td align="left"> <input class="inp" type="password" id="iPass" size="16" maxlength="16"/></td> 
			<td><div class="message errLogin" id="ePass">**PASSWRONG**</div></td>           
		</tr>           
	</table>    

	<p><button class="but" type="button" id="bEnter">**ENTER**</button></p>        
	<p><button class="but" type="button" id="bReg">**REGISTRATION**</button></p>
	       
</div> 